<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".VAXJOWEATHER" >
    
    
    <ImageView
        android:id="@+id/logo"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:src="@drawable/ic_launcher" >
    </ImageView>

    <TextView
        android:id="@+id/IDlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/logo"
        android:paddingLeft="10dp"
        android:paddingTop="20dp"
        android:text="@+id/IDlabel"
        android:textSize="15sp"
        
         />

    <TextView
        android:id="@+id/Datelabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/logo"
        android:text="@+id/Datelabel"
        
         />

    <TextView
        android:id="@+id/Degreeslabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/logo"
        android:text="@+id/Degreeslabel"
        
         />

    <TextView
        android:id="@+id/Rainlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Windlabel"
        android:layout_alignBottom="@+id/Degreeslabel"
        android:layout_alignLeft="@+id/Datelabel"
        
        android:paddingLeft="30dp"
        android:text="@+id/Rainlabel"
        
         />

    <TextView
        android:id="@+id/Windlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Degreeslabel"
        android:layout_alignBottom="@+id/Degreeslabel"
        android:layout_toRightOf="@+id/Degreeslabel"
       
       
        android:layout_marginLeft="16dp"
        android:paddingLeft="30dp"
        android:text="@+id/Windlabel"
        
         />

    <TextView
        android:id="@+id/Stopdatelabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/Datelabel"
        android:layout_below="@+id/Datelabel"
        android:text="@+id/Stopdatelabel"
        
         />
    
    
    
    

</RelativeLayout>
